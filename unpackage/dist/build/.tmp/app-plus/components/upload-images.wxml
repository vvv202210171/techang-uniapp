<view class="upload-content"><block wx:for="{{imageList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="upload-item"><image class="upload-img" src="{{item.filePath}}" mode="aspectFill" data-event-opts="{{[['tap',[['previewImage',[index]]]]]}}" bindtap="__e"></image><image class="upload-del-btn" src="data:image/png;base64," mode="scaleToFill" data-event-opts="{{[['tap',[['delImage',[index]]]]]}}" bindtap="__e"></image><block wx:if="{{item.progress<100}}"><view class="upload-progress">{{item.progress+"%"}}</view></block></view></block></block><block wx:if="{{rduLength>0}}"><view data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="upload-add-btn" bindtap="__e"></view></block></view>