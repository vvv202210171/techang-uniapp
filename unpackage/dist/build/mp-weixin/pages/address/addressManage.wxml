<view class="content"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','receiveName','$event',[]],['addressData']]]]]}}" value="{{addressData.receiveName}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','receivePhone','$event',[]],['addressData']]]]]}}" value="{{addressData.receivePhone}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地址</text><text data-event-opts="{{[['tap',[['showMulLinkageThreePicker',['$event']]]]]}}" class="input" bindtap="__e">{{addressData.address}}</text><text data-event-opts="{{[['tap',[['showMulLinkageThreePicker',['$event']]]]]}}" class="yticon icon-shouhuodizhi" bindtap="__e"></text><mpvue-city-picker class="vue-ref" vue-id="1" pickerValueDefault="{{cityPickerValueDefault}}" data-ref="mpvueCityPicker" data-event-opts="{{[['^onChange',[['changeAddr']]],['^onConfirm',[['changeAddr']]]]}}" bind:onChange="__e" bind:onConfirm="__e" bind:__l="__l"></mpvue-city-picker></view><view class="row b-b"><text class="tit">详细地址</text><input class="input" type="text" placeholder="详细地址" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','detailAddr','$event',[]],['addressData']]]]]}}" value="{{addressData.detailAddr}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{setDefault}}" color="#fa436a" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view><button data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e">提交</button></view>