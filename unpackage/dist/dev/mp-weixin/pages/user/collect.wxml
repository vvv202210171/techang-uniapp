<view class="content"><scroll-view class="list-scroll-content" scroll-y><block wx:if="{{navList.length==0}}"><empty vue-id="1" bind:__l="__l"></empty></block><block wx:for="{{navList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="order-item"><view class="i-top b-b"><text class="time">{{item.goodsName}}</text><text data-event-opts="{{[['tap',[['deleteCollect',['$0'],[[['navList','',index]]]]]]]}}" class="del-btn yticon icon-iconfontshanchu1" bindtap="__e"></text></view><view class="goods-box-single"><image class="goods-img" src="{{item.goodsPicUrl}}" mode="aspectFill" data-event-opts="{{[['tap',[['navToDetailPage',['$0'],[[['navList','',index]]]]]]]}}" bindtap="__e"></image><view class="right"><text class="title clamp">{{"销量："+item.goodsSaleNum}}</text><text class="attr-box">{{"原价："+item.goodsOriginalPrice}}</text><text class="price">{{"现价："+item.goodsPrice}}</text></view></view></view></block></scroll-view></view>